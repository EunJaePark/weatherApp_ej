,EAAE,sBADS;AAElBC,UAAAA,KAAK,EAAE;AAFW,SAApB;AAIA3B,QAAAA,QAAQ,CAAC4B,YAAT,CAAsBH,WAAtB,EAPY,CAQZ;;AACAV,QAAAA,YAAY,CAACc,OAAb,CAAqB,WAArB,EAAkCf,SAAlC;AACD,OAfM,CAAP;AAgBD,KApBM;AAqBPgB,IAAAA,WArBO,uBAqBKlB,OArBL,EAqBcC,QArBd,EAqBwB;AAC7B,aAAOd,YAAY,CAACc,QAAD,CAAZ,CACNI,IADM,CACD,UAAAC,GAAG,EAAI;AACXN,QAAAA,OAAO,CAACO,MAAR,CAAe,WAAf,EAA4BD,GAAG,CAACE,IAAhC;AACA,eAAOF,GAAP;AACD,OAJM,EAKNG,KALM,CAKA,UAAAC,GAAG,EAAI;AACZC,QAAAA,OAAO,CAACC,GAAR,CAAYF,GAAZ;AAED,OARM,CAAP;AASD;AA/BM,GAhBmB;AAiD5BS,EAAAA,OAAO,EAAE;AAjDmB,CAAf,CAAf","sourcesContent":["import Vue from 'vue'\nimport Vuex from 'vuex'\nimport { weatherCity, /*weatherIcon,*/ weather5days } from '../api/index'\nimport { eventbus } from '../main.js';\n\nVue.use(Vuex)\n\nexport default new Vuex.Store({ //const로 선언하는 것과 차이....?\n  state: {\n    weatherData: [],\n    weather5daysData: [],\n  },\n  mutations: {\n    SET_CITY(state, city) {\n      state.weatherData = city\n    },\n    // SET_ICON(state, icon) {\n    //   state.iconData = icon\n    // },\n    SET_5DAYS(state, city) {\n      state.weather5daysData = city\n    },\n  },\n  actions: {\n    FETCH_WEATHER(context, cityname) {\n      // 직전에 검색했던 도시명 불러와 변수 선언.\n      let savedCity = localStorage.getItem('city_name');\n      return weatherCity(cityname)\n      .then(res => {\n        context.commit('SET_CITY', res.data);\n        return res;\n      })\n      .catch(err => {\n        console.log(`직전에 검색했던 도시: ${savedCity}`);\n        // 도시명이 올바르지 않을 경우 알림창 띄움.\n        const sendMessage = {\n          message: '검색 지역을 올바르게 입력해 주세요.',\n          error: true,\n        };\n        eventbus.alertMessage(sendMessage);\n        // 도시명이 올바르지 않을 경우, 직전에 검색했던 도시명을 다시 사용.\n        localStorage.setItem('city_name', savedCity);\n      })\n    },\n    FETCH_5DAYS(context, cityname) {\n      return weather5days(cityname)\n      .then(res => {\n        context.commit('SET_5DAYS', res.data)\n        return res\n      })\n      .catch(err => {\n        console.log(err);\n        \n      })\n    },\n  },\n  modules: {\n  }\n})\n"]}]}